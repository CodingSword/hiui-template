{
  "name": "hiui-template",
  "version": "0.1.0",
  "description": "HIUI Templates",
  "scripts": {
    "test": "node_modules/.bin/standard && node_modules/.bin/stylelint --config .stylelintrc 'src/**/*.scss'",
    "fix": "node_modules/.bin/standard --fix && node_modules/.bin/stylelint --config .stylelintrc 'src/**/*.scss' --fix",
    "eject": "react-scripts eject",
    "build": "react-scripts build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start"
  },
  "author": "HIUI <mi-hiui@xiaomi.com>",
  "license": "MIT",
  "homepage": "http://hiui-group.github.io/hiui-template/",
  "dependencies": {
    "@hi-ui/classic-theme": "^1.0.0-beta.7",
    "@hi-ui/core-css": "^1.0.0",
    "@hi-ui/hiui": "^1.2.1-beta.1",
    "axios": "^0.18.0",
    "classnames": "^2.2.5",
    "node-sass": "^4.9.3",
    "postcss-loader": "^2.1.5",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/es/",
      "/lib/",
      "/dist/",
      "/libs/markdown/code.js",
      "yarn.lock"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "standard": "^12.0.1",
    "stylelint": "^9.9.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.4.0"
  }
}
