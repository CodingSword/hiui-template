{
  "name": "hiui-template",
  "version": "0.1.5",
  "description": "HIUI Template",
  "scripts": {
    "test": "node_modules/.bin/standard && node_modules/.bin/stylelint --config .stylelintrc 'src/**/*.scss'",
    "fix": "node_modules/.bin/standard --fix && node_modules/.bin/stylelint --config .stylelintrc 'src/**/*.scss' --fix",
    "eject": "react-scripts eject",
    "build": "react-scripts build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start"
  },
  "author": "HIUI <mi-hiui@xiaomi.com>",
  "license": "MIT",
  "homepage": "http://hiui-group.github.io/hiui-template/",
  "dependencies": {
    "@hi-ui/classic-theme": "^2.0.0-beta.7",
    "@hi-ui/component-kit": "^0.5.5",
    "@hi-ui/core-css": "^1.0.0",
    "@hi-ui/hiui": "^1.5.0-rc.6",
    "axios": "^0.18.1",
    "classnames": "^2.2.5",
    "echarts": "^4.2.1",
    "echarts-for-react": "^2.0.15-beta.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^2.1.5",
    "react": "^16.8.6",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.6",
    "react-syntax-highlighter": "^10.3.0"
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "react-scripts": "2.1.1",
    "standard": "^12.0.1",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.8.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/es/",
      "/lib/",
      "yarn.lock"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
